{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"shell:allow-open",
		"dialog:default",
		"dialog:allow-message",
		"dialog:allow-save",
		"dialog:allow-ask",
		"updater:default",
		"process:default",
		"process:allow-restart"
	]
}

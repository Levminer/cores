{
	"name": "cores",
	"version": "0.11.0",
	"description": "Modern hardware monitor for Windows.",
	"author": "Levminer",
	"license": "GPL-3.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "node scripts/buildNumber.cjs alpha true && cross-env VITE_CORES_MODE=app vite interface --port 3000",
		"dev:web": "node scripts/buildNumber.cjs alpha true && cross-env VITE_CORES_MODE=client vite interface --port 3001",
		"build:interface": "node scripts/buildNumber.cjs alpha false && vite build interface",
		"build:library": "cargo build --manifest-path library/Cargo.toml --release && node scripts/copyLib.js",
		"check": "svelte-check && tsc --noEmit"
	},
	"devDependencies": {
		"@baileyherbert/tinro": "^1.0.0",
		"@crownframework/svelte-error-boundary": "^1.0.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@tailwindcss/forms": "^0.5.6",
		"autoprefixer": "^10.4.16",
		"cross-env": "^7.0.3",
		"postcss": "^8.4.32",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-preprocess": "^5.1.2",
		"tailwindcss": "^3.3.6",
		"typescript": "^5.3.3",
		"vite": "^5.0.8",
		"vite-tsconfig-paths": "^4.2.2"
	},
	"overrides": {
		"@rgossiaux/svelte-headlessui": {
			"svelte": "$svelte"
		},
		"svelte-chartjs": {
			"svelte": "$svelte"
		}
	},
	"dependencies": {
		"@rgossiaux/svelte-headlessui": "^1.0.2",
		"chart.js": "^4.4.1",
		"chartjs-plugin-stacked100": "^1.5.2",
		"lucide-svelte": "^0.294.0",
		"peerjs": "^1.5.2",
		"socket.io-client": "^4.7.2",
		"svelte-bootstrap-icons": "^3.1.0",
		"svelte-chartjs": "^3.1.2"
	}
}
